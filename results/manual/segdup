(spec : a b c d
    (spec
        (: a d)
        (dupl
            (: a b c)
            (: a b d)
        )
    )
    (dupl
        (spec
            (: a b c d)
            (: a b c)
        )
        (spec
            (: b c d)
            (spec
                (: b d)
                (: d)
            )
        )
    )
)
